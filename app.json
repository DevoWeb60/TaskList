{
  "expo": {
    "name": "To Devo List",
    "slug": "ToDevoList",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/TaskList.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "devolist.ipa"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/TaskList.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "devolist.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
